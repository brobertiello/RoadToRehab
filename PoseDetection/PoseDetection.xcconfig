// Product Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.example.PoseDetection

// Info.plist location - using the standard location
INFOPLIST_FILE = Info.plist

// Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 16.0

// Swift Version
SWIFT_VERSION = 5.0

// Architectures
ARCHS = $(ARCHS_STANDARD)
VALID_ARCHS = arm64

// Other settings
TARGETED_DEVICE_FAMILY = 1
SUPPORTS_MACCATALYST = NO
SUPPORTS_MAC_DESIGNED_FOR_IPHONE_IPAD = NO 